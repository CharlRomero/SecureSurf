{
  "manifest_version": 3,
  "name": "SecureSurf",
  "version": "0.1",
  "description": "IA Phising",
  "permissions": ["activeTab", "storage"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "background": {
    "service_worker": "background.js"
  }
}
